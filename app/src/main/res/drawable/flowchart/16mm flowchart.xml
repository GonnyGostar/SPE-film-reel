<?xml version="1.0" encoding="utf-8"?>

<flowchart name="16mm Identification Guide">
    <node option="16mm Film" label="16mm Film" question="Is there picture on the film?" id="1" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
        <node option="Yes" label="16mm Film" question="Is the film perforated on both sides?" id="2" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
            <node option="Yes" label="16mm Film" question="Is there a single row of images?" id="3" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                <node option="Yes" label="Double perf standard 16" question="Is the picture black and white?" id="4" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                    <node option="Yes" label="Black and white film" question="Is the picture negative?" id="5" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                        <node option="Yes" label="Black and white negative" question="Does image have line structure?" id="6" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                            <node option="Yes" label="Tele-recording negative" id="7" img="@drawable/placeholder.png" info="@drawable/placeholder.html"> </node>
                            <node option="No" label="Could be camera negative, duplicate negative or internegative" id="8" img="@drawable/placeholder.png" info="@drawable/placeholder.html"> </node> 
                        </node>
                        <node option="No" label="Black and white film" question="Does the film have clear edges?" id="9" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                            <node option="Yes" label="Black and white positive" question="Does it have black footage numbers?" id="10" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                                <node option="Yes" label="Duplicating Positive" id="11" img="@drawable/placeholder.png" info="@drawable/placeholder.html"> </node>
                                <node option="No" label="Black and white positive" id="12" img="@drawable/placeholder.png" info="@drawable/placeholder.html"> </node>
                            </node>    
                            <node option="No" label="Black and white film" question="Does the film have black edges?" id="13" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                                <node option="Yes" label="Black and white film" question="Does edge print read KODACOLOR?" id="14" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                                    <node option="Yes" label="Lenticular film" id="15" img="@drawable/placeholder.png" info="@drawable/placeholder.html"> </node>
                                    <node option="No" label="Black and white reversal" id="16" img="@drawable/placeholder.png" info="@drawable/placeholder.html"> </node>
                                </node>    
                                <node option="No" label="Tinted black and white or blue base TV positive" id="17" img="@drawable/placeholder.png" info="@drawable/placeholder.html"> 
                                </node>
                            </node>
                        </node>
                    </node>
                    <node option="No" label="Colour film" question="Does the film have an orange colour?" id="18" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                        <node option="Yes" label="Colour film" question="Is the picture negative?" id="19" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                            <node option="Yes" label="Colour negative" question="Does the film have black edges?" id="20" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                                <node option="Yes" label="Colour Reversal Intermediate (CRI)" id="21" img="@drawable/placeholder.png" info="@drawable/placeholder.html"> </node>
                                <node option="No" label="Colour negative" question="Does the film contain joins?" id="22" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                                    <node option="Yes" label="Original cut negative" question="Are alternate scenes joined to black leader?" id="23" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                                        <node option="Yes" label="Colour negative A &amp; B rolls" id="24" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                                        </node>
                                        <node option="No" label="Original cut negative" id="25" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                                        </node>
                                    </node>
                                    <node option="No" label="Colour negative" question="Do scene changes have flash frames?" id="26" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                                        <node option="Yes" label="Original negative" id="27" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                                        </node>
                                        <node option="No" label="Duplicate negative or internegative" id="28" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                                        <!--EXTEND?-->
                                        </node>
                                    </node>
                                </node>
                            </node>
                            <node option="No" label="Colour Interpositive" id="29" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                            </node>
                        </node>
                        <node option="No" label="Original negative" question="Does it have black edges?" id="30" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                            <node option="Yes" label="Colour Reversal" question="Does the film have a soundtrack?" id="31" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                                <node option="Yes" label="Combined colour reversal print" question="Can you see reseau pattern?" id="32" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                                    <node option="Yes" label="Dufaycolor" id="33" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                                    </node>
                                    <node option="No" label="Colour reversal mute print or camera master" id="34" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                                    </node>
                                </node>
                                <node option="No" label="Colour Reversal" question="Is there more than one reel for each film?" id="35" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                                    <node option="Yes" label="A &amp; B roll masters" id="36" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                                    </node>
                                    <node option="No" label="Colour reversal master or silent print" question="Can you see reseau pattern?" id="37" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                                        <node option="Yes" label="Dufaycolor" id="38" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                                        </node>
                                        <node option="No" label="Colour reversal mute print or camera master" id="39" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                                        </node>
                                    </node>
                                </node>
                            </node>
                            <node option="No" label="Colour film" question="Is the picture negative?" id="40" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                                <node option="Yes" label="Unmasked negative" id="41" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                                </node>
                                <node option="No" label="Colour positive" question="Is the soundtrack and the edge printing black and white?" id="42" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                                    <node option="Yes" label="Technicolor" id="43" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                                    </node>
                                    <node option="No" label="Colour positive" question="Is the soundtrack blue?" id="44" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                                        <node option="Yes" label="Eastman Color, Agfacolor, Gevacolor, Fujicolor, Ferrania or 3M" id="45" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                                        </node>
                                        <node option="No" label="Orwo or Sovcolor" id="46" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                                        </node>
                                    </node>
                                </node>
                            </node>
                        </node>
                    </node>
                </node>
                <node option="No" label="1:4 double 8mm" id="47" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                </node>
            <node option="No" label="16mm Film" question="Does film have single row of perforations on one side?" id="48" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                <node option="Yes" label="16mm Film" question="Does the image extend to one edge of the film?" id="49" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                    <node option="Yes" label="Super 16" question="Is the picture black and white?" id="50" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                        <node option="Yes" label="Black and white film" question="Is the picture negative?" id="51" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                            <node option="Yes" label="Black and white negative" id="52" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                            </node>
                            <node option="No" label="Black and white film" question="Does the film have clear edges?" id="53" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                                <node option="Yes" label="Black and white positive" question="Does it have black footage numbers?" id="54" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                                    <node option="Yes" label="Duplicating positive" id="55" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                                    </node>
                                    <node option="No" label="Black and white positive" id="56" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                                    </node>
                                </node>
                                <node option="No" label="Black and white reversal" id="57" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                                </node>
                            </node>
                        </node>
                        <node option="No" label="Colour film" question ="Does the film have an orange colour?" id="58" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                            <!-- SAME AS ID 19, REDIRECTING THERE-->
                            <node option="Yes" goto-node="19">
                            </node>
                            <node option="No" label="Colour film" question ="Does it have black edges?" id="59" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                                <node option="Yes" label="Colour Reversal" question ="Is there more than one reel for each film?" id="60" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                                    <node option="Yes" label="A&amp;B roll masters" id="61" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                                    </node>
                                    <node option="No" label="Colour reversal master or silent print" id="62" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                                    </node>
                                </node>
                                <node option="No" label="Colour positive" id="63" img="@drawable/placeholder.png" info="@drawable/placeholder.html">
                                </node>
                            </node>
                        </node>
                    </node>
                    <!-- SAME AS ID 4, REDIRECTING THERE-->
                    <node option="No" goto-node="4">
                    </node>
                </node>
            </node>
        </node>
    </node>
</flowchart>